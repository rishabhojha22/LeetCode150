class Solution {
    public int maxProfit(int[] prices) {
        int min=prices[0];
        int maxProfit=0;
        int i=1,j=prices.length-1;
        while(i<=j){
            if(prices[i]<min){
                min=prices[i];
            }
            int currProfit=prices[i]-min;
            maxProfit=Math.max(maxProfit,currProfit); 
            i++;           
        }
        return maxProfit;
    }
}

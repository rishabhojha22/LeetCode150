class Solution {
    public int rob(int[] nums) {
        if(nums.length==1)return nums[0];
        if(nums.length==2){
            return Math.max(nums[0],nums[1]);
        }

        int prev2=nums[0];
        int prev1=Math.max(prev2,nums[1]);
        int len=nums.length;

        for(int i=2;i<len;i++){
            int temp=Math.max(prev1,prev2+nums[i]);
            prev2=prev1;
            prev1=temp;
        }

        return prev1;
    }
}
